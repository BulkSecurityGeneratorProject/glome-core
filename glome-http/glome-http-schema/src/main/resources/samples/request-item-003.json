{
  "request" : {
    "url" : {
      "@type" : "composite-url",
      "scheme" : "http",
      "host" : "localhost",
      "port" : "${PORT_NUMBER}",
      "path" : "account",
      "query" : "accountId=12345"
    },
    "method" : "POST",
    "headers" : [ {
      "key" : "Accept",
      "value" : "text/html"
    }, {
      "key" : "Accept-Language",
      "value" : "en-US,en;q=0.8"
    } ],
    "requestBody" : {
      "@type" : "raw-body",
      "raw" : "Body${BODY_CONTENT}"
    }
  }
}