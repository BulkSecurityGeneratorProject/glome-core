{
  "request" : {
    "url" : {
      "@type" : "composite-url",
      "scheme" : "${SCHEMA}",
      "host" : "httpbin.org",
      "port" : "${PORT_NUMBER}",
      "path" : "/post"
    },
    "method" : "POST",
    "headers" : [ {
      "key" : "Accept",
      "value" : "text/html"
    }, {
      "key" : "Accept-Language",
      "value" : "en-US,en;q=0.8"
    } ],
    "requestBody" : {
      "@type" : "raw-body",
      "raw" : "${BODY_CONTENT}"
    }
  }
}