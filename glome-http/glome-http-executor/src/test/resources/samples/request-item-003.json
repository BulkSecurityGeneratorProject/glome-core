{
  "request" : {
    "url" : {
      "@type" : "raw-url",
      "raw" : "${URL}"
    },
    "method" : "POST",
    "headers" : [ {
      "key" : "Accept",
      "value" : "text/html"
    }, {
      "key" : "Accept-Language",
      "value" : "en-US,en;q=0.8"
    } ],
    "requestBody" : {
      "@type" : "file-body",
      "filePath" : "src/test/resources/samples/${BODY_FILE_NAME}"
    }
  }
}